{
  "preset": "google",
  "disallowSpacesInAnonymousFunctionExpression": null,
  "validateLineBreaks": "LF",
  "validateIndentation": 2,
  "maximumLineLength": 1000,
  "excludeFiles": ["./client.js", "./src/CometD/*.js", "node_modules/**"],
  "jsDoc": {
    "checkAnnotations": {
      "preset": "jsdoc3",
      "extra": {
        "test": true
      }
    },
    "checkTypes": "capitalizedNativeCase"
  }
}
